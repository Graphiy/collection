{
  "name": "@graphiy/collection",
  "version": "0.1.1",
  "description": "Eventful collection",
  "keywords": [
    "array data model"
  ],
  "homepage": "https://github.com/graphiy/collection#readme",
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Adrian Dmitra",
    "email": "adrian@dmitra.com"
  },
  "main": "src/collection.js",
  "module": "src/collection.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Graphiy/collection"
  },
  "scripts": {
    "doc": "jsdoc src -r -d docs",
    "lint": "eslint .",
    "pretest": "yarn lint",
    "test": "jest"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ],
            "node": "current"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "env": {
      "mocha": true
    },
    "extends": "@graphiy/eslint-config",
    "globals": {
      "_": true,
      "$": true
    }
  },
  "dependencies": {
    "eventemitter3": "^3.0.3"
  },
  "devDependencies": {
    "@graphiy/eslint-config": "^0.1.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "jest": "^23.1.0",
    "jsdoc": "^3.5.3",
    "mocha": "^5.2.0"
  },
  "jest": {
    "testMatch": [
      "**/test/**/*.js"
    ]
  },
  "peerDependencies": {
    "lodash": "^4.x.x"
  }
}
